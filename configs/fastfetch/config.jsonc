		{
			"type": "wm",
			"key": " WM ",
			"keyColor": "yellow"
		},
		{
			"type": "terminal",
			"key": "│ ├ ",
			"keyColor": "yellow"
		},
		{
			"type": "terminalfont",
			"key": "└ └󰛖 ",
			"keyColor": "yellow"
		},
		{
			"type": "shell",
