			"key": "└ └󰹑 ",
			"keyColor": "red"
		},
		{
			"type": "custom",
			"format": "\u001b[90m└────────────────────────────────────────────────────┘"
		},
		{
			"type": "custom",
			"format": "\u001b[90m┌──────────────────────Software──────────────────────┐"
		},
		{
			"type": "os",
			"key": "󰣇 OS ",
			"keyColor": "blue"
		},
		{
			"type": "kernel",
