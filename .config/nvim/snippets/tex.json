{
  "template": {
    "prefix": "\\template",
    "body": [
      "\\documentclass{${3:article}}",
      "\\title{$1}",
      "\\author{$2}",
      "\\date{\\today}",
      "",
      "\\begin{document}",
      "\\maketitle",
      "$4",
      "\\end{document}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "environment": {
    "prefix": "beg",
    "body": [
      "\\begin{$1}\n\t${2}\n\\end{$1}"
    ],
    "description": "Environment",
    "luasnip": {
      "priority": 2000,
      "autotrigger": true
    }
  },
  "table of contents": {
    "prefix": "\\toc",
    "body": [
      "\\tableofcontents"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "usepackage": {
    "prefix": "\\pack",
    "body": [
      "\\usepackage[$2]{$1}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "french": {
    "prefix": "\\french",
    "body": [
      "\\usepackage[T1]{fontenc}",
      "\\usepackage[french]{babel}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "section": {
    "prefix": "\\sec",
    "body": [
      "\\section{$1}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "math": {
    "prefix": "[",
    "body": [
      "\\[$1\\"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "inline math": {
    "prefix": "$",
    "body": [
      "$$1$"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "fraction": {
    "prefix": "\\frac",
    "body": [
      "\\frac{$1}{$2}"
    ],
    "luasnip": {
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "equation": {
    "prefix": "\\eq",
    "body": [
      "\\begin{equation}",
      "$1",
      "\\end{equation}"
    ],
    "luasnip": {
      "autotrigger": true
    }
  },
  "mathbb": {
    "prefix": "\\bb",
    "body": [
      "\\mathbb{$1}"
    ],
    "luasnip": {
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "mathcal": {
    "prefix": "\\cal",
    "body": [
      "\\mathcal{$1}"
    ],
    "luasnip": {
      "autotrigger": true,
      "wordTrig": false
    }
  },
  "mathfrak": {
    "prefix": "\\frak",
    "body": [
      "\\mathfrak{$1}"
    ],
    "luasnip": {
      "autotrigger": true,
      "wordTrig": false
    }
  }
}
